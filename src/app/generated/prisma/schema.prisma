generator client {
  provider = "prisma-client-js"
  output   = "../src/app/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    String  @id
  name  String
  image String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
